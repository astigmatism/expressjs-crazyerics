doctype html
html
  head
    script.
        var masterFile = !{masterFile};
        var system = '!{system}';
        var paths = !{paths};
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.4/jquery.min.js')
    script(src='/javascripts/isotope.min.js')
    script(src='/javascripts/dev/mediabrowser.js', type='text/javascript')
    script(src='/javascripts/imagesloaded.min.js')
    script(src='/javascripts/dropzone.js', type='text/javascript')
    link(rel='stylesheet', href='/stylesheets/dropzone.css')
    link(rel='stylesheet', href='/stylesheets/dev/mediabrowser.css')
    link(rel='stylesheet', type='text/css', href='http://fonts.googleapis.com/css?family=Open+Sans|Poppins|Kanit')
  body
    div#header
      div#searchwrapper
        | Search: 
        input#search
      div#filterwrapper
        | Filter By Rank:
        input(type="checkbox")#hidered 
        | Hide Red
        input(type="checkbox")#hideyellow
        | Hide Yellow
        input(type="checkbox")#hidegreen
        | Hide Green
    ul#list