	{
		"1.6.9-stable":
		{
			"notes": "works decent for most systems. cant get n64, 32x or genesis (weird I know). Recalbox uses this version (summer 2018) which suggests its performant.",
			"config":
			{
				"system_directory": "/system",
				"input_menu_toggle":  "f1",
				"savestate_directory": "/states",
				"video_font_enable": false,				// # Enable usage of OSD messages.
				"video_shader_enable": true,
				"input_shader_next": "rightbracket",
				"input_shader_prev": "leftbracket",
				"video_shader_dir": "/shaders",
				"screenshot_directory": "/screenshots",
				"savefile_directory": "/saves",
				"input_screenshot": "t",
				"video_gpu_screenshot": false,
				"input_exit_emulator": "escape",
				"input_save_state": "num1",
				"input_load_state": "num4",
				"input_state_slot_increase": "num3",
				"input_state_slot_decrease": "num2",
				"input_audio_mute": "m",
				"input_reset": "h",
				"input_toggle_fullscreen": "f",
				"input_hold_fast_forward": "space",
				"input_player1_select": "shift",
				"input_cheat_toggle": "u",
				"audio_latency": 16,
				"audio_sync": true,
				"audio_out_rate": 44100, 				//48000 default. Got better perf when lower, less crackling
				"rewind_enable": true,
				"rewind_buffer_size": 100,
				"input_frame_advance": "nul",
				"video_force_aspect": true,
				"video_vsync": true,				// (when false, fps dropped noticibly)
				"video_hard_sync": false,			//# Attempts to hard-synchronize CPU and GPU. Can reduce latency at cost of performance. (Seems to have no effect when true)
				"video_scale": 5, 					// (above 5 i have best looking image, effects some shaders)
				"video_fullscreen": false,
				"video_windowed_fullscreen": false, //# If fullscreen, prefer using a windowed fullscreen mode. (I had better luck with false)
